{
  "version": 2,
  "devCommand": "uvicorn main:app --reload",
  "buildCommand": "bun run build",
  "installCommand": "bun install",
  "framework": "sveltekit",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./bun.lock",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.py"
    },
    {
      "src": "/(.*)/",
      "status": 308,
      "headers": { "Location": "/$1" }
    },
    {
      "src": "/(.*)\\.(\\w+)",
      "dest": "/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/$1.html"
    }
  ],
  "functions": {
    "api/main.py": {
      "maxDuration": 60,
      "memory": 1024
    }
  }
}